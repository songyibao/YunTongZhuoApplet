.item_center{
  display: flex;
  justify-content: center;
  align-items: center;
}
.top_border{
  width: 100%;
  height: 2rpx;
  border-bottom: 2rpx solid rgb(199, 199, 199);
  margin-bottom: 20rpx;
  font-family: 'zhehei';
  color: #87C5D3;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 0rpx 0rpx 15rpx 3rpx rgb(199, 199, 199);
}
.page_holder{
  width: 100%;
  height: 100%;
  padding: 3%;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.box_shadow {
  box-shadow: 0px 0rpx 15rpx 1rpx rgb(199, 199, 199);
}
@font-face {
  font-family: 'zhehei';
  src: url('data:font/ttf;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
@font-face {
  font-family: 'siyuanhei';
  src: url('data:font/ttf;charset=utf-8;base64,') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.mask{
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.5);
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.txt{
  border-radius: 20rpx;
  width: 80%;
  height: 200rpx;
  font-size: 28rpx;
  color: #333;
  background: #fff;
  margin: 20% auto 50rpx;
  padding: 20rpx;
}

.sub-btn{
  border-radius: 20rpx;
  margin: 0rpx 20rpx 0rpx 20rpx;
  width: 40%;
  height: 80rpx;
  line-height: 80rpx;
  background: #ffffff;
  font-size: 28rpx;
  color: rgb(0, 0, 0);
  display: flex;
  justify-content: center;
  align-items: center;
}